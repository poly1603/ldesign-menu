{
  "name": "@ldesign/menu-core",
  "version": "0.1.0",
  "description": "LDesign Menu Core - Framework-agnostic menu system core logic",
  "keywords": [
    "ldesign",
    "menu",
    "core",
    "navigation",
    "framework-agnostic",
    "typescript"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./core": {
      "types": "./es/core/index.d.ts",
      "import": "./es/core/index.js",
      "require": "./lib/core/index.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./features": {
      "types": "./es/features/index.d.ts",
      "import": "./es/features/index.js",
      "require": "./lib/features/index.cjs"
    },
    "./features/*": {
      "types": "./es/features/*.d.ts",
      "import": "./es/features/*.js",
      "require": "./lib/features/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    },
    "./es/index.css": "./es/index.css",
    "./lib/index.css": "./lib/index.css",
    "./dist/index.umd.css": "./dist/index.umd.css",
    "./dist/index.umd.min.css": "./dist/index.umd.min.css"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "dev": "ldesign-builder build --mode development --watch",
    "clean": "rimraf es lib dist coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@ldesign/builder": "link:../../../../tools/builder",
    "typescript": "^5.7.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  }
}